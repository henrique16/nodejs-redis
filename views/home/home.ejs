<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>

<body>
    <h2>Event By User <%= eventsByUser[0].idUser %></h2>
    <div id="eventsByUser">
        <%
            if(eventsByUser.length > 0) { 
                eventsByUser.forEach(eventByUser => { %>
                    <%- include("../component/eventByUser.ejs", { eventByUser: eventByUser }) %>
        <%      }) 
            } 
        %>
    </div>
    <h2>Event</h2>
    <div id="events">
        <% 
            if(events.length > 0) {
                events.forEach(event => { %>
                   <%- include("../component/event.ejs", { event: event }) %>
        <%      })
            } 
        %>
    </div>
    <div>
        <input id="renderCreateEvent" type="submit" value="Create event" onclick="renderCreateEvent()">
    </div>
    <%- include("script.ejs") %>
</body>

</html>